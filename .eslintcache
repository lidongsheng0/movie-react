   title:'vue',\r\n        content: 'Vue 则引入了模版，将很多实现封装成了 API，你需要记住并调用 API 来进行开发，因为很多都是封装好的，所以学习起来较为简单，只是编程的感觉稍微弱了一些。',\r\n        id: uuidv4(),\r\n        completed: false\r\n    },\r\n]\r\nexport default function App() {\r\n    const [blogList, setBlogList] = useState(initialBlogList);\r\n    const [search, setSearch] = useState('');\r\n    const [edit, setEdit] = useState(false);\r\n    const [id, setId] = useState(null);\r\n\r\n    const toggleEdit = (id) => {\r\n        setEdit(true);\r\n        setId(id);\r\n    }\r\n    const addBlog = (title,content) => {\r\n        const newItem = {\r\n            title,\r\n            content,\r\n            id: uuidv4(),\r\n            collected: false\r\n        };\r\n        setBlogList(blogList => blogList.concat(newItem));\r\n    }\r\n    const collectBlog = (id) => {\r\n        setBlogList(blogList => {\r\n            return blogList.map(blog => {\r\n                if(id===blog.id) {\r\n                    return {\r\n                        ...blog,\r\n                        collected: !blog.collected\r\n                    };\r\n                }\r\n                return blog;\r\n            })\r\n        })\r\n    }\r\n    const deleteBlog = (id) => {\r\n        setBlogList(blogList => {\r\n            return blogList.filter(blog => blog.id !== id);\r\n        })\r\n    }\r\n    const filterBiogList = (filter,BiogList) => {\r\n        return BiogList.filter(biog => biog.content.indexOf(filter) !== -1);\r\n    }\r\n    const editBlog = () => {\r\n        if(edit) {\r\n            const blog = blogList.filter(blog => blog.id === id);\r\n            return blog[0];\r\n        }\r\n    }\r\n    return (\r\n        <Container maxWidth='md'>\r\n            <Typography variant='h3' color='primary' align='center'>\r\n                博客\r\n            </Typography>\r\n\r\n            <AddBlog addBlog={addBlog} edit={edit} editBlog={editBlog()} setEdit={setEdit}/>\r\n\r\n            <Box className='main-search'>\r\n                <FormControl margin='normal'>\r\n                    <Input placeholder='搜索' endAdornment={\r\n                        <InputAdornment>\r\n                            <SearchIcon />\r\n                        </InputAdornment>\r\n                    } value={search} onInput={(e)=> setSearch(e.target.value)}/>\r\n                </FormControl>\r\n            </Box>\r\n            <Box my={4}>\r\n                <Divider />\r\n            </Box>\r\n          \r\n            <BlogList blogList={filterBiogList(search,blogList)} onCollect={collectBlog} onDelete={deleteBlog} onEdit={toggleEdit}/>\r\n        </Container>\r\n    )\r\n}\r\n",["122","123"],"D:\\Study\\react\\demo_3\\src\\blog\\BlogList.js",[],"D:\\Study\\react\\demo_3\\src\\blog\\AddBlog.js",["124"],"import React,{useEffect,useRef,useState} from 'react'\r\nimport {FormControl,Input,InputLabel,Button} from '@material-ui/core'\r\n\r\nexport default function AddBlog({ addBlog,edit,editBlog,setEdit }) {\r\n    \r\n    const [title, setTitle] = useState('');\r\n    const [content, setContent] = useState('');\r\n    const titleRef = useRef(null);\r\n    const contentRef = useRef(null);\r\n\r\n    const handleInput = (e) => {\r\n        if(e.target.name==='title') {\r\n            setTitle(e.target.value);\r\n        }else if(e.target.name==='content') {\r\n            setContent(e.target.value)\r\n        }\r\n    }\r\n    const handleSubmit = (e) => {\r\n        e.preventDefault();\r\n        if(!title.trim() || !content.trim()) {\r\n            if(!title.trim()) {\r\n                alert('请输入标题');\r\n                titleRef.current.focus();\r\n                return;\r\n            }\r\n            if(!content.trim()) {\r\n                alert('请输入内容');\r\n                contentRef.current.focus()\r\n                return;\r\n            }\r\n        }\r\n        addBlog(title,content);\r\n        setContent('');\r\n        setTitle('');\r\n        titleRef.current.focus();\r\n    }\r\n    useEffe